<!-- _koujues.html.erb -->

<% eid ||= 'koujueTid' %>
<select id="<%= eid %>" name="<%= eid %>" class="form-control">
  <% CocsProxy.instance.getGameConfig('koujue').each do |tid, value| %>
     <option value='<%= tid %>'>
      <%= 
        name = GameConfig.strings[value['name']] || value['name'] 
        "#{name}"
      %>
    </option>
  <% end %>
</select>