{
  "always_prompt_for_file_reload": false,
  "auto_indent": true,
  "caret_style": "smooth",
  "detect_indentation": true,
  "file_exclude_patterns":
  [
    "*.meta",
    "*.pyc",
    "*.pyo",
    "*.exe",
    "*.dll",
    "*.obj",
    "*.o",
    "*.a",
    "*.lib",
    "*.so",
    "*.dylib",
    "*.ncb",
    "*.sdf",
    "*.suo",
    "*.pdb",
    "*.idb",
    ".DS_Store",
    "*.class",
    "*.psd",
    "*.db",
    "*.sublime-workspace",
    "*.csproj",
    ".tags",
    ".tags_sorted_by_file",
    ".gemtags",
    "*.ccbi",
    "*.cer",
    "*.pem",
    "*.p12",
    "*.png",
    "*.jpg",
    "*.xls",
    "*.ttf",
    "*.GlyphProject",
    "*.dat",
    "*.pvr.ccz",
    "*.mark",
    "*.oldmark",
    "*.ccbi"
  ],
  "flatland_sidebar_tree_small": true,
  "flatland_square_tabs": false,
  "folder_exclude_patterns":
  [
    ".svn",
    ".git",
    ".hg",
    "CVS",
    "metadata",
    "ScriptAssemblies",
    "ShaderCache",
    "Library",
    "ProjectSettings"
  ],
  "font_options":
  [
    "no_bold",
    "no_italic",
    "subpixel_antialias"
  ],
  "highlight_line": true,
  "hot_exit": false,
  "ignored_packages":
  [
    "Makefile",
    "Vintage"
  ],
  "line_padding_bottom": 2,
  "line_padding_top": 3,
  "remember_open_files": false,
  "show_full_path": true,
  "smart_indent": true,
  "tab_size": 2,
  "use_tab_stops": true,
  "translate_tabs_to_spaces": true,
  "trim_automatic_white_space": true,
  "trim_trailing_white_space_on_save": true
}
